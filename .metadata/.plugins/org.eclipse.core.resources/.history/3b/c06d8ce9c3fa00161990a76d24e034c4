#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#include <string.h>


int main(void) {
	srand(time(NULL));


	FILE * rand_num_file = fopen("file.txt", "w");
	if (rand_num_file == NULL)
	{
	    printf("Error opening file!\n");
	    exit(1);
	}


	return EXIT_SUCCESS;
}


