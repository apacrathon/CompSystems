#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int32_t generate_randnum_file(int32_t n, char * PATH);

int main(void) {

	generate_randnum_file(0, "./");
	return EXIT_SUCCESS;
}

int32_t generate_randnum_file(int32_t n, char * PATH)
{
	int result = 0;

	if (strcmp(PATH, "") == 0) { printf("Setting n to default value %s", PATH = "./";); }
	if (n <= 0) { printf("Setting n to default value %d", n = 1); }




	return result;
}
