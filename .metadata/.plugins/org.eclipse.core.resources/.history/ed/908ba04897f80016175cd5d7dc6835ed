#include <stdlib.h>

int print_pid();
int system();

int main(void)
{
	//system();
}

int print_pid()
{
	printf("The process ID is %d\n", (int) getpid());
	printf("The parent process ID %d\n", (int) getppid());
	return 0;
}

int system()
{
	int return_value = system("ls -l /");
	return return_value;
}
